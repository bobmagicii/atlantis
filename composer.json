{
	"require": {
		"netherphp\/world": "^3.0.1",
		"google\/recaptcha": "^1.1",
		"robmorgan\/phinx": "^0.6",
		"netherjs/nui": "^3.0",
		"artem_c/emmet": "^1.0",
		"netherphp/standards": "^0.1.8",
		"squizlabs/php_codesniffer": "^3.5",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
		"ramsey/uuid": "^4.0"
	},
	"config": {
		"preferred-install": {
			"netherphp\/*": "source",
			"*": "dist"
		}
	},
	"autoload": {
		"psr-0": {
			"Atlantis\\": "core\/",
			"Nether\\": "core\/"
		},
		"psr-4": {
			"Routes\\": "routes\/"
		}
	}
}
